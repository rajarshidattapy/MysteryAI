{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/auth",
      "dest": "/auth"
    },
    {
      "src": "/gameStart",
      "dest": "/gameStart"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}